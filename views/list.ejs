<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lists</title>
    <link rel="stylesheet" href="css/styles.css">
    
</head>
<body>
    
    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>
    
    <div class="box">
        
            <%for(var i=0;i<newListItem.length;i++){%>
                <div class="item">
                    <input type="checkbox">
                   <p><%= newListItem[i] %></p> 
            
                </div>
             <% } %>
       
    

    <!-- Form for Activity parsing -->

    <form class="item" action="/" method="post"> 
          <input type="text"  id="" name="newList" placeholder="Enter the next Activity" >
        
        <button type="submit" name="list" value="<%= listTitle %>">+</button>
      </form>
    </div>


</body>
</html>